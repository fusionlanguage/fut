{
  "name": "fusion",
  "version": "3.0.2",
  "publisher": "fusionlanguage",
  "license": "MIT",
  "displayName": "Fusion",
  "description": "Fusion language syntax highlighting",
  "icon": "fu-logo.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/fusionlanguage/fut.git"
  },
  "engines": {
    "vscode": "^1.0.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "keywords": [
    "fusion",
    "fut",
    "transpiler"
  ],
  "contributes": {
    "languages": [
      {
        "id": "fusion",
        "extensions": [
          ".fu"
        ],
        "icon": {
          "light": "./fu-file.svg",
          "dark": "./fu-file.svg"
        },
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "fusion",
        "scopeName": "source.fu",
        "path": "./syntaxes/fusion.tmLanguage.json"
      }
    ]
  },
  "activationEvents": [
    "onLanguage:fusion"
  ],
  "main": "./out/extension.js",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ."
  },
  "devDependencies": {
    "@types/vscode": "^1.0.0",
    "typescript": "^5.2.2"
  }
}
