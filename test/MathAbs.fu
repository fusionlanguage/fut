	{
		int i = Math.Abs(-1000);
		long l = -1000000000;
		int j = Math.Abs(l);
		l *= i;
		float f = -10.5;
		f = Math.Abs(f);
		double d = Math.Abs(-1e200); //FAIL: cl if no double support
